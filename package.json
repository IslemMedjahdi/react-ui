{
  "name": "@aboutbits/react-ui",
  "version": "1.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/aboutbits/react-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aboutbits/react-ui.git"
  },
  "homepage": "https://storybook.aboutbits.it",
  "engines": {
    "npm": "^8",
    "node": "^16"
  },
  "scripts": {
    "dev": "start-storybook -p 4000",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project tsconfig.build.esm.json",
    "build:cjs": "tsc --project tsconfig.build.cjs.json",
    "lint": "eslint --ext .js,.ts,.tsx,.jsx,.stories.mdx,.docs.mdx,.json .",
    "lint:fix": "eslint --ext .js,.ts,.tsx,.jsx,.stories.mdx,.docs.mdx,.json --fix .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "build-storybook": "build-storybook",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "version": "npm run test && npm run lint && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@aboutbits/pagination": "^1.1.0",
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@aboutbits/internationalization": "^0.2.1",
    "@aboutbits/react-material-icons": "^1.1.2",
    "@aboutbits/react-pagination": "^0.0.10",
    "@aboutbits/react-toolbox": "^0.1.2",
    "@babel/core": "^7.18.5",
    "@hookform/resolvers": "^2.9.8",
    "@reach/dialog": "^0.17.0",
    "@reach/menu-button": "^0.17.0",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.5.12",
    "@tailwindcss/forms": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.3",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "autoprefixer": "^10.4.12",
    "axios": "^0.27.2",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.2.5",
    "cssnano": "^5.1.12",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-mdx": "^1.17.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.6",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "next": "^12.1.6",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^7.36.1",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.7.4",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "react-hook-form": "^7.36.1"
  },
  "peerDependenciesMeta": {
    "react-hook-form": {
      "optional": true
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "tailwind-preset.js"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./tailwind-preset.js": "./tailwind-preset.js",
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js"
    },
    "./framework": {
